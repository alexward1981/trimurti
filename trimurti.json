                "editable": false,
                "readable": true,
                "default": null,
                "validation": null
            },
            "website": {
                "type": "string",
                "required": true,
                "editable": false,
                "readable": true,
                "default": null,
                "validation": null
            },
            "company": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "catchPhrase": {
                        "type": "string"
                    },
                    "bs": {
                        "type": "string"
                    }
                },
                "required": true,
                "editable": false,
                "readable": true,
                "default": null,
                "validation": null
            }
        }
    }
