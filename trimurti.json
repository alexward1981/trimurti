{
    "appName": "TestApp",
    "apiRoot": "https://jsonplaceholder.typicode.com",
    "routes": {
        "name": "users",
        "data": {
            "id": {
                "type": "number",
                "required": true,
                "editable": false,
                "readable": true,
                "default": null,
                "validation": null
            },
            "name": {
                "type": "string",
                "required": true,
                "editable": false,
                "readable": true,
                "default": null,
                "validation": null
            },
            "username": {
                "type": "string",
                "required": true,
                "editable": false,
                "readable": true,
                "default": null,
                "validation": null
            },
            "email": {
                "type": "string",
                "required": true,
                "editable": false,
                "readable": true,
                "default": null,
                "validation": null
            },
            "address": {
                "type": "object",
                "properties": {
                    "street": {
                        "type": "string"
                    },
                    "suite": {
                        "type": "string"
                    },
                    "city": {
                        "type": "string"
                    },
                    "zipcode": {
                        "type": "string"
                    },
                    "geo": {
                        "type": "object",
                        "properties": {
