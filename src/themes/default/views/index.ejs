<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Trimurti editor</title>
  <link rel="stylesheet" href="/static/styles/core.min.css">
</head>
<body>
  <div class="wrapper">
    <div class="tree_menu">
      <ul>
        <% config.routes.forEach(function(route) { %>
          <li>
            <%= route.name %>
          </li>
        <% }) %>
        <li class="settings">
          Settings
        </li>
      </ul>
    </div>
    <div class="editor">
      <h1>Currently editing:</h1>
      <h2><%= RouteName %></h2>
    </div>
  </div>
</body>
</html>
